{
  "env": {
    "node": true,
    "es2021": true
  },
  "extends": [
    "plugin:react/recommended",
    "airbnb",
    "airbnb/hooks",
    "prettier"
    // "prettier/react"
  ],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react", "react-native"],
  "rules": {
    // "react/require-default-props": false
    // "react/prop-types": true
    "react/prop-types": 0,
    "react/jsx-filename-extension": 0,
    "react/no-unstable-nested-components": 0,
    "no-unused-vars":"warn"
  }
}
